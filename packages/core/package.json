{
  "name": "@scrapito/core",
  "version": "0.0.1",
  "description": "Scraping template core",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "browser": {
    "fs": false,
    "./dist/index.esm.js": "./dist/browser.js",
    "./dist/workers/axios.js": "./dist/browser-workers/axios.js"
  },
  "exports": {
    ".": {
      "node": {
        "module": "./dist/index.esm.js",
        "require": "./dist/index.cjs.js",
        "import": "./dist/index.esm.js"
      },
      "default": "./dist/browser.js"
    },
    "./package.json": "./package.json"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -cw",
    "local": "npm run build && npm i -g && scrapito",
    "version": "lerna version --yes",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Pulsar Development Team",
  "license": "MIT",
  "dependencies": {
    "@types/puppeteer": "^5.4.3",
    "axios": "^0.21.1",
    "decoders": "^1.23.4",
    "js-yaml": "^4.0.0",
    "threads": "^1.6.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.34",
    "rollup": "^2.55.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "threads-plugin": "^1.4.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "eslintConfig": {
    "extends": "../../.eslintrc.js"
  },
  "optionalDependencies": {
    "puppeteer": "^8.0.0"
  }
}
